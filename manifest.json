{

  "manifest_version": 2,
  "name": "WebToPDF",
  "version": "1.0",

  "description": "Adds a browser action icon to the toolbar. Click the button to download as pdf",
  "icons": {
    "48": "icons/logo.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "WebToPDF",
    "default_popup": "popup/download.html"
  }

}